<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="382.0" prefWidth="433.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.inviaggio.LoginAmministratoreController">
   <children>
      <VBox alignment="CENTER" prefHeight="298.0" prefWidth="305.0">
         <children>
            <Label text="Login Amministratore">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Pane prefHeight="259.0" prefWidth="324.0">
               <children>
                  <Label layoutX="173.0" layoutY="62.0" text="Codice Admin">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="codiceAmministratore" layoutX="142.0" layoutY="89.0" />
                  <Label fx:id="pinErrato" layoutX="170.0" layoutY="121.0" text="Inserire pin valido" textFill="RED" />
                  <Button fx:id="bottoneConferma" layoutX="303.0" layoutY="209.0" mnemonicParsing="false" onAction="#onConfermaClick" prefHeight="25.0" prefWidth="107.0" text="Conferma" />
                  <Button fx:id="bottoneAnnulla" layoutX="29.0" layoutY="209.0" mnemonicParsing="false" onAction="#onAnnullaClick" prefHeight="25.0" prefWidth="107.0" text="Annulla" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</StackPane>
